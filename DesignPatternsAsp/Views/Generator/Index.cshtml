<div class="row">
    <div class="col-8 offset-2">
        <div class="form-group">
            <select id="opFile" class="form-control">
                <option value="1">Json</option>
                <option value="2">Pipes</option>
            </select>
        </div>
        <div class="form-group">
            <button class="btn btn-success" onclick="generate();">Generar Archivo</button>
        </div>
    </div>
</div>

<script>
    function generate(){
        
        let opFile = document.getElementById("opFile").value;

        fetch("@Url.Content("~/Generator/CreateFile")" + "?opFile=" + opFile)
            .then(res => res.json())
            .then(data => alert(data));
    }
</script>